<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.aryad.sipasar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/frame_1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ModulProfilDanRiwayatLapak">
        <activity android:name=".SplashScreenActivity" android:theme="@style/Theme.ModulProfilDanRiwayatLapak.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".LoginPegawaiActivity"
            android:theme="@style/Theme.ModulProfilDanRiwayatLapak.NoActionBar"/>
        <activity
            android:screenOrientation="portrait"
            android:name=".LapakProfileActivity" android:theme="@style/Theme.ModulProfilDanRiwayatLapak.NoActionBar"/>
        <activity
            android:screenOrientation="portrait"
            android:name=".ListLapakActivity"
            android:theme="@style/Theme.ModulProfilDanRiwayatLapak.NoActionBar"></activity>
    </application>

</manifest>